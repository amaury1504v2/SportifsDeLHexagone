ercises: response.data })\n      })\n      .catch((error) => {\n        console.log(error);\n      })\n  }\n\n  deleteExercise(id) {\n    axios.delete('http://localhost:5000/exercises/'+id)\n      .then(response => { console.log(response.data)});\n\n    this.setState({\n      exercises: this.state.exercises.filter(el => el._id !== id)\n    })\n  }\n\n  exerciseList() {\n    return this.state.exercises.map(currentexercise => {\n      return <Exercise exercise={currentexercise} deleteExercise={this.deleteExercise} key={currentexercise._id}/>;\n    })\n  }\n\n  render() {\n    return (\n      <div>\n        <h3>Activités</h3>\n        <table className=\"table\">\n          <thead className=\"thead-light\">\n            <tr>\n              <th>Nom d'utilisatuer</th>\n              <th>Description</th>\n              <th>Durée</th>\n              <th>Date</th>\n              <th>Activités</th>\n            </tr>\n          </thead>\n          <tbody>\n            { this.exerciseList() }\n          </tbody>\n        </table>\n      </div>\n    )\n  }\n}","/Users/amaurygimonnet/Desktop/Sites/mern-exercise-tracker/src/components/create-exercise.js",[],"/Users/amaurygimonnet/Desktop/Sites/mern-exercise-tracker/src/components/create-user.js",[],"/Users/amaurygimonnet/Desktop/Sites/mern-exercise-tracker/src/components/edit-exercise.js",[],"/Users/amaurygimonnet/Desktop/Sites/mern-exercise-tracker/src/components/welcome.js",[],{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","severity":1,"message":"48","line":12,"column":91,"nodeType":"49","endLine":12,"endColumn":189},"no-native-reassign",["50"],"no-negated-in-lhs",["51"],"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plug